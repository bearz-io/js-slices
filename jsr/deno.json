{
    "name": "@bearz/slices",
    "version": "0.1.1",
    "description": "Cross runtime module for slices and char slices which creates views over arrays without allocating  a new array.  Char slices is useful for transforming text without allocating a new string for every change.",
    "keywords": [
        "slices",
        "arrays",
        "char-arrays",
        "characters",
        "case-insensitive",
        "string"
    ],
    "exports": {
        ".": "./mod.ts",
        "./camelize": "./camelize.ts",
        "./capitalize": "./capitalize.ts",
        "./char-array-builder": "./char_array_builder.ts",
        "./char-slice": "./char_slice.ts",
        "./dasherize": "./dasherize.ts",
        "./ends-with": "./ends_with.ts",
        "./equal": "./equal.ts",
        "./index-of": "./index_of.ts",
        "./last-index-of": "./last_index_of.ts",
        "./ordinalize": "./ordinalize.ts",
        "./pascalize": "./pascalize.ts",
        "./slice": "./slice.ts",
        "./starts-with": "./starts_with.ts",
        "./titleize": "./titleize.ts",
        "./tokens": "./tokens.ts",
        "./trim": "./trim.ts",
        "./underscore": "./underscore.ts",
        "./utils": "./utils.ts"
    }
}
